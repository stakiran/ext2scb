{
	"name": "open-scb",
	"displayName": "open scb",
	"description": "open xxxx.scb from xxxx.ext.",
	"publisher": "stakiran",
	"repository": "https://github.com/stakiran/openscb",
	"version": "0.0.1",
	"license": "MIT",
	"engines": {
		"vscode": "^1.70.0"
	},
	"main": "./out/extension.js",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:openscb.neworopen"
	],
	"contributes": {
		"commands": [
			{
				"command": "openscb.neworopen",
				"title": "New or Open .scb file from the current",
				"category": "openscb"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "openscb.neworopen",
					"when": "resourceExtname != .scb"
				}
			],
			"editor/context": [
				{
					"command": "openscb.neworopen",
					"group": "navigation@20",
					"when": "resourceExtname != .scb"
				}
			]
		},
		"keybindings": [
			{
				"command": "openscb.neworopen",
				"key": "shift+enter",
				"mac": "shift+enter",
				"when": "resourceExtname != .scb"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"lint": "eslint --ext .ts ./src",
		"pretty": "prettier ./src/*.ts --write"
	},
	"devDependencies": {
		"@types/vscode": "^1.70.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"vscode-test": "^1.4.1",
		"@vscode/test-electron": "^2.1.5"
	}
}
